{% extends 'account/base_account.html.twig' %}
{% block main %}
    <!-- MAIN -->
    <main>
        <div class="head-title">
            <div class="left">
                <h1>Dashboard</h1>
                <ul class="breadcrumb">
                    <li>
                        <a href="#">Dashboard</a>
                    </li>
                    <li><i class='bx bx-chevron-right' ></i></li>
                    <li>
                        <a class="active" href="#">Profile</a>
                    </li>
                </ul>
            </div>

        </div>




        <div class="table-data">
            {% for message in app.flashes('success') %}
                <div class="alert alert-success">
                    {{ message }}
                </div>
            {% endfor %}
            {% for message in app.flashes('error') %}
                <div class="alert alert-success">
                    {{ message }}
                </div>
            {% endfor %}
            <div class="todo">
                <div class="head">
                    <h3>Par√©metrer votre profil</h3>
                </div>

                <hr>
                {% if notification %}
                    <div class="alert alert-info">{{ notification }}</div>
                {% endif %}
                <form method="POST" action="{{ path('app_profil') }}" enctype="multipart/form-data">
                    {{ form_widget(form) }}

                </form>
                <hr>


            </div>
        </div>
    </main>

{% endblock %}

